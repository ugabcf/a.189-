#include <iostream>
using namespace std;
/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char** argv) {
	int N,S=0,L;
	cin>>N;
	while(N!=0)                  //數字還沒變成0的時候
	{
	L=N%10;                      // N%10 L 存    N/10的餘數 S存L  N/10 重複進行
	S=S+L;
	N=N/10;
	}
	cout<<S;
}
